{
  "expo": {
    "name": "VirtueMaster",
    "slug": "virtuemaster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/custom-pngs/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/custom-pngs/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0d35e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/custom-pngs/logo.png",
        "backgroundColor": "#4ac69b"
      }
    },
    "web": {
      "favicon": "./assets/custom-pngs/logo.png"
    }
  }
}
